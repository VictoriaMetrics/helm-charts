    metadata:
      labels:
        app: vlstorage
        app.kubernetes.io/instance: RELEASE-NAME
        app.kubernetes.io/managed-by: Helm
        app.kubernetes.io/name: victoria-logs-cluster
        app.kubernetes.io/version: 0.1.0
        helm.sh/chart: victoria-logs-cluster-0.1.1
      name: RELEASE-NAME-victoria-logs-cluster-vlstorage
      namespace: NAMESPACE
    spec:
      minReadySeconds: 5
      podManagementPolicy: OrderedReady
      replicas: 2
      selector:
        matchLabels:
          app: vlstorage
          app.kubernetes.io/instance: RELEASE-NAME
          app.kubernetes.io/name: victoria-logs-cluster
      serviceName: RELEASE-NAME-victoria-logs-cluster-vlstorage
      template:
        metadata:
          labels:
            app: vlstorage
            app.kubernetes.io/instance: RELEASE-NAME
            app.kubernetes.io/managed-by: Helm
            app.kubernetes.io/name: victoria-logs-cluster
        spec:
          containers:
            - args:
                - --envflag.enable
                - --http.shutdownDelay=15s
                - --httpListenAddr=:9491
                - --loggerFormat=json
                - --retentionPeriod=7d
                - --storageDataPath=/storage
              image: victoriametrics/victoria-logs:0.1.0
              imagePullPolicy: IfNotPresent
              name: vlstorage
              ports:
                - containerPort: 9491
                  name: http
              readinessProbe:
                failureThreshold: 3
                httpGet:
                  path: /health
                  port: http
                  scheme: HTTP
                initialDelaySeconds: 5
                periodSeconds: 5
                timeoutSeconds: 5
              securityContext:
                runAsGroup: 1000
                runAsNonRoot: true
                runAsUser: 1000
              volumeMounts:
                - mountPath: /storage
                  name: vlstorage-volume
          securityContext:
            fsGroup: 1000
          terminationGracePeriodSeconds: 60
      volumeClaimTemplates:
        - apiVersion: v1
          kind: PersistentVolumeClaim
          metadata:
            labels:
              app: vlstorage
              app.kubernetes.io/instance: RELEASE-NAME
              app.kubernetes.io/managed-by: Helm
              app.kubernetes.io/name: victoria-logs-cluster
              app.kubernetes.io/version: 0.1.0
              helm.sh/chart: victoria-logs-cluster-0.1.1
            name: vlstorage-volume
          spec:
            accessModes:
              - ReadWriteOnce
            resources:
              requests:
                storage: 10Gi
