ingress should match snapshot:
  1: |
    apiVersion: networking.k8s.io/v1
    kind: Ingress
    metadata:
      annotations:
        kubernetes.io/ingress.class: nginx
      labels:
        app: vmauth
        app.kubernetes.io/instance: RELEASE-NAME
        app.kubernetes.io/managed-by: Helm
        app.kubernetes.io/name: vt-cluster
        app.kubernetes.io/version: v1.116.0
        helm.sh/chart: victoria-traces-cluster-0.1.1
      name: RELEASE-NAME-vt-cluster-vmauth
      namespace: NAMESPACE
    spec:
      rules:
        - host: vtselect.node.local
          http:
            paths:
              - backend:
                  service:
                    name: RELEASE-NAME-vt-cluster-vmauth
                    port:
                      name: http
                path: /select/test
                pathType: Prefix
