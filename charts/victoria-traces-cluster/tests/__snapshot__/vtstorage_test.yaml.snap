      name: RELEASE-NAME-vt-cluster-vtstorage
      namespace: NAMESPACE
    spec:
      minReadySeconds: 5
      podManagementPolicy: OrderedReady
      replicas: 2
      selector:
        matchLabels:
          app: vtstorage
          app.kubernetes.io/instance: RELEASE-NAME
          app.kubernetes.io/name: vt-cluster
      serviceName: RELEASE-NAME-vt-cluster-vtstorage
      template:
        metadata:
          labels:
            app: vtstorage
            app.kubernetes.io/instance: RELEASE-NAME
            app.kubernetes.io/managed-by: Helm
            app.kubernetes.io/name: vt-cluster
        spec:
          containers:
            - args:
                - --envflag.enable
                - --http.shutdownDelay=15s
                - --httpListenAddr=:10491
                - --loggerFormat=json
                - --retentionPeriod=7d
                - --storageDataPath=/storage
              image: victoriametrics/victoria-traces:0.1.0
              imagePullPolicy: IfNotPresent
              name: vtstorage
              ports:
                - containerPort: 10491
                  name: http
              readinessProbe:
                failureThreshold: 3
                httpGet:
                  path: /health
                  port: http
                  scheme: HTTP
                initialDelaySeconds: 5
                periodSeconds: 5
                timeoutSeconds: 5
              securityContext:
                runAsGroup: 1000
                runAsNonRoot: true
                runAsUser: 1000
              volumeMounts:
                - mountPath: /storage
                  name: vtstorage-volume
          securityContext:
            fsGroup: 1000
          terminationGracePeriodSeconds: 60
      volumeClaimTemplates:
        - apiVersion: v1
          kind: PersistentVolumeClaim
          metadata:
            name: vtstorage-volume
          spec:
            accessModes:
              - ReadWriteOnce
            resources:
              requests:
                storage: 10Gi
